<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../partials/head'); %>
  </head>
  <body class="container">
    <header><%- include('../partials/header'); %></header>

    <main>
      <div class="jumbotron">
        <h1>환영합니다</h1>
        <p><%= tagline %></p>
        <ul>
          <% members.forEach(function(members) { %>
          <li><strong><%= members.name %></strong> : <%=members.role%></li>
          <% }); %>
        </ul>
      </div>
    </main>

    <footer><%- include('../partials/footer'); %></footer>
  </body>
</html>
